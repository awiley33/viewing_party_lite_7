<h1>User Registration Page</h1><br><br>

<%= form_with(
  model: @user,
  url: users_path,
  method: :post,
  local: true,
  data: { turbo: false }
  ) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.submit "Register" %>
  <% end %>